{
  "name": "webgeo",
  "version": "1.0.0",
  "description": "webgeo",
  "main": "dist/index.html",
  "author": "gissehel",
  "license": "MIT",
  "scripts": {
    "s": "cross-env NODE_ENV=development parcel serve ./src/index.html",
    "watch": "cross-env NODE_ENV=development parcel watch ./src/index.html",
    "build-leg": "cross-env NODE_ENV=production parcel build ./src/index.html --no-minify",
    "build-gh": "cross-env NODE_ENV=production parcel build ./src/index.html --no-minify",
    "b": "cross-env NODE_ENV=production parcel build ./src/index.html --no-source-maps --dist-dir ./dist/",
    "latlonparser": "run-s latlonparser:jison latlonparser:patch",
    "latlonparser:jison": "jison src/client/utils/latlon/latlonparser.jison -o src/client/utils/latlon/latlonparser.js -m commonjs",
    "latlonparser:patch": "node -e \"const fs = require('fs'); const fp = './src/client/utils/latlon/latlonparser.js'; const d = fs.readFileSync(fp); fs.writeFileSync(fp, '/* eslint-disable */\\n' + d + '\\nexport default latlonparser;\\n');\"",
    "jison": "jison",
    "clean": "rm -rf ./.parcel-cache && rm -rf ./dist && ls -la",
    "test-jest": "cross-env NODE_ENV=test jest",
    "hygen": "hygen",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "crypto-js": "^4.0.0",
    "express": "5.1.0",
    "history": "^5.3.0",
    "leaflet": "^1.6.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-helmet": "^6.1.0",
    "react-leaflet": "^2.7.0",
    "react-redux": "9.1.0",
    "react-router-dom": "6",
    "redux": "5.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-onboarding": "8.6.14",
    "@storybook/blocks": "8.6.14",
    "@storybook/experimental-addon-test": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/react-vite": "8.6.14",
    "@storybook/test": "8.6.14",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/browser": "^3.1.4",
    "@vitest/coverage-v8": "^3.1.4",
    "hygen": "^6.0.4",
    "jest-html-reporter": "^3.2.0",
    "jison": "^0.4.18",
    "playwright": "^1.52.0",
    "prop-types": "15.8.1",
    "run-s": "^0.0.0",
    "semantic-ui-react": "3.0.0-beta.2",
    "storybook": "8.6.14",
    "vite": "^6.3.5",
    "vitest": "^3.1.4",
    "yarn-run-all": "^3.1.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
